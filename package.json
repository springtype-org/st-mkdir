{
  "name": "st-mkdir",
  "version": "1.0.0-beta.1",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "springtype mkdir",
    "st-mkdir",
    "mkdir"
  ],
  "description": "CLI tool for platform independent recursive creating directories",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/springtype-org/springtype.git"
  },
  "scripts": {
    "build": "st-run st-rm-rf dist ++ system:tsc ++ st-cp README.md package.json node_modules dist",
    "test": "ts-node test.ts"
  },
  "engines": {
    "node": ">=10"
  },
  "bundledDependencies": [
    "chalk"
  ],
  "bin": {
    "st-mkdir": "./cli.js"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Aron Homberg, Michael Mannseicher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/springtype-org/springtype/issues"
  },
  "homepage": "https://springtype.org",
  "dependencies": {
    "chalk": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.13.2",
    "node": "^14.0.0",
    "st-run": "^2.0.0-beta.3",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
